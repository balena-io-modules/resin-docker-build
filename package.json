    "@types/duplexify": "^3.6.1",
    "@types/event-stream": "^4.0.0",
    "@types/js-yaml": "^4.0.1",
    "@types/jsonstream": "^0.8.30",
    "@types/klaw": "^3.0.3",
    "@types/lodash": "^4.14.181",
    "@types/mocha": "^9.1.0",
    "@types/mz": "^2.7.4",
    "@types/rewire": "^2.5.28",
    "@types/semver": "^7.3.5",
    "@types/tar-stream": "^2.2.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "js-yaml": "^4.1.0",
    "mocha": "^9.2.2",
    "ncp": "^2.0.0",
    "rewire": "^6.0.0",
