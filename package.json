    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "files": [
    "build/*.js",
    "build/*.js.map",
    "build/*.d.ts",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "author": "Cameron Diver <cameron@resin.io>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resin-io/resin-docker-build.git"
  },
  "bugs": {
    "url": "https://github.com/resin-io/resin-docker-build/issues"
  },
  "homepage": "https://github.com/resin-io/resin-docker-build#readme",
  "dependencies": {
    "@types/bluebird": "^3.0.37",
    "@types/dockerode": "^2.3.1",
    "@types/klaw": "^1.3.1",
    "@types/lodash": "^4.14.51",
    "@types/mz": "0.0.30",
    "@types/node": "^8.10.0",
    "JSONStream": "^1.3.0",
    "bluebird": "^3.4.7",
    "dockerode": "^2.3.6",
    "duplexify": "^3.5.0",
    "klaw": "^1.3.1",
    "lodash": "^4.17.4",
    "mz": "^2.6.0",
    "tar-stream": "^1.5.2"
  },
  "devDependencies": {
    "@types/mocha": "^5.0.0",
    "@types/through": "^0.0.29",
    "mocha": "^5.0.5",
    "resin-lint": "^1.5.0",
    "ts-node": "^5.0.1",
    "typescript": "^2.2.1"
  }
