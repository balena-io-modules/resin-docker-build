{
  "name": "resin-docker-build",
  "version": "1.0.0",
  "description": "A containerised builder which interacts with the docker remote API to perform builds.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc --project tsconfig.publish.json",
    "build:test": "npm run clean && tsc --project .",
    "clean": "rm -rf build",
    "lint": "resin-lint --typescript src/ test/ && tsc --noEmit",
    "prepublish": "require-npm4-to-publish",
    "prepublishOnly": "npm run build",
    "pretest": "npm run lint",
    "prettify": "prettier --config ./node_modules/resin-lint/config/.prettierrc --write \"{src,test,typings}/**/*.ts\"",
    "test": "npm run build:test && mocha build/test"
  },
  "author": "Cameron Diver <cameron@resin.io>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resin-io/resin-docker-build.git"
  },
  "bugs": {
    "url": "https://github.com/resin-io/resin-docker-build/issues"
  },
  "homepage": "https://github.com/resin-io/resin-docker-build#readme",
  "dependencies": {
    "@types/bluebird": "3.5.20",
    "@types/dockerode": "2.5.5",
    "@types/duplexify": "3.6.0",
    "@types/event-stream": "^3.3.34",
    "@types/jsonstream": "^0.8.30",
    "@types/klaw": "^1.3.1",
    "@types/lodash": "^4.14.51",
    "@types/mz": "0.0.30",
    "@types/node": "^10.12.11",
    "@types/tar-stream": "1.6.0",
    "JSONStream": "^1.3.0",
    "bluebird": "^3.4.7",
    "dockerode": "^2.5.5",
    "duplexify": "^3.5.0",
    "event-stream": "^3.3.5",
    "klaw": "^1.3.1",
    "lodash": "^4.17.4",
    "mz": "^2.6.0",
    "tar-stream": "^1.6.2"
  },
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.39",
    "@types/rewire": "^2.5.28",
    "chai": "^3.5.0",
    "dts-generator": "^2.1.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-mocha": "^4.1.0",
    "gulp-shell": "^0.6.1",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-typescript": "^3.1.5",
    "gulp-util": "^3.0.8",
    "mocha": "^3.2.0",
    "require-npm4-to-publish": "^1.0.0",
    "resin-lint": "^3.0.1",
    "rewire": "^4.0.1",
    "ts-node": "^2.1.0",
    "typescript": "^3.2.1"
  }
}
