{
  "name": "resin-docker-build",
  "version": "1.1.6",
  "description": "A containerised builder which interacts with the docker remote API to perform builds.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rimraf build",
    "lint": "npm run lint:lib && npm run lint:tests && tsc --noEmit",
    "lint:lib": "balena-lint --typescript lib/ typings/",
    "lint:tests": "balena-lint --typescript --tests test/",
    "copy:all": "true",
    "build": "npm run clean && tsc --project ./ && npm run copy:all",
    "test": "npm run lint && ts-mocha --project ./tsconfig.test.json test/*.ts",
    "prepublishOnly": "npm run build && npm test",
    "prettify": "balena-lint --typescript --fix src/ test/"
  },
